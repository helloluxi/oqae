 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "getDeltaLowerBound", ",", "getDeltaQpeSin", ",", "getDeltaQpeUnf"}], 
   "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"getDeltaLowerBound", "[", "eps_", "]"}], ":=", 
   RowBox[{
    RowBox[{"getDeltaLowerBound", "[", "eps", "]"}], "=", 
    RowBox[{"getFiniteDeltaBound", "[", 
     RowBox[{"256", ",", "eps"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"getDeltaQpeSin", "[", 
    RowBox[{"eps_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"getDeltaQpeSin", "[", 
     RowBox[{"eps", ",", "n"}], "]"}], "=", 
    RowBox[{"1", "-", 
     RowBox[{"Sum", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        FractionBox["2", 
         RowBox[{"n", 
          RowBox[{"(", 
           RowBox[{"n", "+", "1"}], ")"}]}]], 
        RowBox[{"NIntegrate", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             FractionBox[
              RowBox[{
               RowBox[{"Sin", "[", 
                FractionBox["\[Pi]", 
                 RowBox[{"n", "+", "1"}]], "]"}], 
               RowBox[{"Cos", "[", 
                RowBox[{
                 FractionBox["1", "2"], 
                 RowBox[{"(", 
                  RowBox[{"n", "+", "1"}], ")"}], "th"}], "]"}]}], 
              RowBox[{"2", 
               RowBox[{"Sin", "[", 
                RowBox[{
                 FractionBox["1", "2"], 
                 RowBox[{"(", 
                  RowBox[{"th", "+", 
                   FractionBox["\[Pi]", 
                    RowBox[{"n", "+", "1"}]]}], ")"}]}], "]"}], " ", 
               RowBox[{"Sin", "[", 
                RowBox[{
                 FractionBox["1", "2"], 
                 RowBox[{"(", 
                  RowBox[{"th", "-", 
                   FractionBox["\[Pi]", 
                    RowBox[{"n", "+", "1"}]]}], ")"}]}], "]"}]}]], ")"}], 
            "2"], "/.", 
           RowBox[{"th", "->", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"ArcCos", "[", 
               RowBox[{
                RowBox[{"2", "x"}], "-", "1"}], "]"}], "-", 
              FractionBox[
               RowBox[{"2", "Pi", " ", "k"}], "n"]}], ")"}]}]}], 
          "\[IndentingNewLine]", ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"Max", "[", 
             RowBox[{
              RowBox[{
               SuperscriptBox[
                RowBox[{"Cos", "[", 
                 FractionBox[
                  RowBox[{"Pi", " ", "k"}], "n"], "]"}], "2"], "-", 
               FractionBox["eps", "n"]}], ",", "0"}], "]"}], ",", 
            RowBox[{"Min", "[", 
             RowBox[{
              RowBox[{
               SuperscriptBox[
                RowBox[{"Cos", "[", 
                 FractionBox[
                  RowBox[{"Pi", " ", "k"}], "n"], "]"}], "2"], "+", 
               FractionBox["eps", "n"]}], ",", "1"}], "]"}]}], "}"}], 
          "\[IndentingNewLine]", ",", 
          RowBox[{"PrecisionGoal", "\[Rule]", "4"}]}], "]"}]}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "0", ",", 
         RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}]}]}], ";"}]}], "Input",
 CellLabel->"In[18]:=",ExpressionUUID->"d8413283-a364-4888-beec-522964efcba2"],

